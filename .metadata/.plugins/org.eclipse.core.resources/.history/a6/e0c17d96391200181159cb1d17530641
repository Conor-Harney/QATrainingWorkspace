import java.util.Scanner;

public class Interface {
	
	   Scanner m_scanner; 
	   
	   public Interface()
	   {
		   m_scanner = new Scanner(System.in);
	   }
	   
	   
	   
	   public String taveInput()
	   {
		   System.out.println("what do you want");
		   return  m_scanner.nextLine();
	   }
	   

}
